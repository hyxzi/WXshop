.index_cate {
  height: 100%;
}
.index_cate .cate_container {
  height: calc(100vh - 90rpx);
  display: flex;
  /* 左边商品导航样式 */
  /* 右边商品列表样式 */
}
.index_cate .cate_container .left_menu {
  flex: 2;
}
.index_cate .cate_container .left_menu .menu_item {
  height: 80rpx;
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-content: center;
}
.index_cate .cate_container .active {
  color: var(--themeColor);
}
.index_cate .cate_container .right_menu {
  flex: 5;
}
.index_cate .cate_container .right_menu .goods_group .goods_title {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-content: center;
}
.index_cate .cate_container .right_menu .goods_group .goods_title .delimiter {
  color: #ccc;
  padding: 0 10rpx;
}
.index_cate .cate_container .right_menu .goods_group .goods_list {
  display: flex;
  flex-wrap: wrap;
}
.index_cate .cate_container .right_menu .goods_group .goods_list navigator {
  width: 33.33%;
  text-align: center;
}
.index_cate .cate_container .right_menu .goods_group .goods_list navigator image {
  width: 50%;
}
